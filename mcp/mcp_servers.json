{
    "mcpServers": {
        "excel": {
            "command": "uvx",
            "args": ["excel-mcp-server", "stdio"],
            "env": {}
        },
        "time": {
            "command": "uvx",
            "args": ["mcp-server-time"],
            "env": {}
        },
        "slack": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-slack"],
            "env": {
                "SLACK_BOT_TOKEN": "xoxb-your-bot-token",
                "SLACK_TEAM_ID": "your-team-id"
            }
        }
    }
}

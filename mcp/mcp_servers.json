{
    "mcpServers": {
        "excel": {
            "command": "uvx",
            "args": ["excel-mcp-server", "stdio"],
            "env": {}
        },
        "time": {
            "command": "uvx",
            "args": ["mcp-server-time"],
            "env": {}
        },
        "slack": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-slack"],
            "env": {
                "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}",
                "SLACK_TEAM_ID": "${SLACK_TEAM_ID}",
                "SLACK_CHANNEL_IDS": "C09H5KW3475"
            }
        }
    }
}
